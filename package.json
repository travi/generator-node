    "yeoman-generator": "0.24.1"
  },
  "devDependencies": {
    "@travi/any": "0.8.0",
    "cucumber": "2.0.0-rc.1",
    "greenkeeper-postpublish": "1.0.1",
    "mockery": "2.0.0",
